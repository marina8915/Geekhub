
<install type="module" version="1.5.0">
	<name>DWContactMap</name>
	<version>1.03.0006</version>
	<description><![CDATA[<h1>DW Contact Map</h1>
    <p>Google Map for Joomla! contacts.</p>
    <p>Place this module on Joomla! contacts detail page to display Google Map for contact address.</p>
    <p><strong>Note!</strong> To use Google Maps, you must <a href="http://code.google.com/apis/maps/" target="_blank">obtain Google API Key</a> for your site.</p>
    <p>DW Contact Map developed by <a href="http://factory.docwriter.ru/" target="_blank">Factory.DocWriter.Ru</a>.</p>]]></description>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<author>Factory.DocWriter.Ru</author>
	<authoremail>info@docwriter.ru</authoremail>
	<authorurl>http://factory.docwriter.ru</authorurl>
	<copyright>Copyright (c) 2009-2010 Factory.DocWriter.Ru</copyright>
	<creationDate>16-May-10</creationDate>
    <files folder="site">
        <folder>tmpl</folder>
        <filename>CHANGELOG.php</filename>
        <filename>helper.php</filename>
        <filename module="mod_dwcontactmap">mod_dwcontactmap.php</filename>
		<filename>index.html</filename>
    </files>
    <languages folder="site/language">
		<language tag="en-GB">en-GB.mod_dwcontactmap.ini</language>
		<language tag="ru-RU">ru-RU.mod_dwcontactmap.ini</language>
	</languages>
	<params>
		<param name="gmap_id" type="text" default="" label="Google Maps ID" description="GMAP_ID_DESC" />
		<param name="height" type="text" default="250" label="Height" description="HEIGHT_DESC" />
		<param name="canvas_style" type="text" default="" label="Canvas Style" description="CANVAS_STYLE_DESC" />
		<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="PARAMMODULECLASSSUFFIX" />
	</params>
	<params group="advanced">
		<param name="cache" type="list" default="1" label="Caching" description="Select whether to cache the content of this module">
			<option value="1">Use global</option>
			<option value="0">No caching</option>
		</param>
		<param name="cache_time" type="text" default="900" label="Cache Time" description="The time before the module is recached" />
	</params>
</install>